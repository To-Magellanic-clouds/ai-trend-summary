    "   \"source\": [\n",
    "    \"# 统计文章链接和标题\\n\",\n",
    "    \"article_dict= {}\\n\",\n",
    "    \"articles = info_div.find_elements(By.CSS_SELECTOR, \\\"article\\\")  # 或 By.XPATH, \\\".//article\\\")\\n\",\n",
    "    \"for article in articles:\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        link = article.find_element(By.TAG_NAME, \\\"a\\\")\\n\",\n",
    "    \"        url = link.get_attribute(\\\"href\\\")\\n\",\n",
    "    \"        title = link.find_element(By.TAG_NAME, \\\"h4\\\").text\\n\",\n",
    "    \"        article_dict[url] = title\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"Error: {e}\\\")\\n\",\n",
    "    \"print(article_dict)\"\n",
