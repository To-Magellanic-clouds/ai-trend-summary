    "   \"source\": [\n",
    "    \"# 单个文章打开测试\\n\",\n",
    "    \"first = next(iter(article_dict))\\n\",\n",
    "    \"print(first)\\n\",\n",
    "    \"driver.get(\\\"https://huggingface.co/blog/Suzana/interactive-tools\\\")\\n\",\n",
    "    \"# 定位正文容器\\n\",\n",
    "    \"content_div = driver.find_element(By.XPATH, \\\"/html/body/div/main/div/div[1]\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 提取所有子元素（包括标题、段落、列表等）\\n\",\n",
    "    \"elements = content_div.find_elements(By.XPATH, \\\".//*\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 存储结构化文本\\n\",\n",
    "    \"markdown_text = \\\"\\\"\\n\",\n",
    "    \"for element in elements:\\n\",\n",
    "    \"    tag_name = element.tag_name\\n\",\n",
    "    \"    text = element.text.strip()\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if tag_name.startswith(\\\"h\\\") and text:  # 处理标题（h1, h2, h3...）\\n\",\n",
    "    \"        level = int(tag_name[1])  # 提取标题级别（如 h1 -> 1）\\n\",\n",
    "    \"        markdown_text += f\\\"{'#' * level} {text}\\\\n\\\\n\\\"\\n\",\n",
    "    \"    elif tag_name == \\\"p\\\" and text:  # 处理段落\\n\",\n",
    "    \"        markdown_text += f\\\"{text}\\\\n\\\\n\\\"\\n\",\n",
    "    \"    elif tag_name in [\\\"ul\\\", \\\"ol\\\"]:  # 处理列表\\n\",\n",
    "    \"        items = element.find_elements(By.TAG_NAME, \\\"li\\\")\\n\",\n",
    "    \"        for item in items:\\n\",\n",
    "    \"            markdown_text += f\\\"- {item.text}\\\\n\\\"\\n\",\n",
    "    \"        markdown_text += \\\"\\\\n\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"print(markdown_text)\"\n",
